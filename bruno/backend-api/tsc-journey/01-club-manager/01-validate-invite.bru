meta {
  name: 01 Validate Invite
  type: http
  seq: 1
}

get {
  url: {{api_url}}/api/invites/validate/{{club_manager_invite_code}}
  auth: none
}

tests {
  test("should validate club manager invite or be used/expired", function() {
    expect([200, 404]).to.include(res.status);
    if (res.status === 200) {
      expect(res.body.invite_type).to.equal('club_manager');
    }
  });
}
