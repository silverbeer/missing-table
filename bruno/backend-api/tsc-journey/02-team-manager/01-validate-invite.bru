meta {
  name: 01 Validate Team Manager Invite
  type: http
  seq: 1
}

get {
  url: {{api_url}}/api/invites/validate/{{team_manager_invite_code}}
  auth: none
}

assert {
  res.status: eq 200
}

tests {
  test("should validate team manager invite", function() {
    expect(res.status).to.equal(200);
    expect(res.body.invite_type).to.equal('team_manager');
  });
}
